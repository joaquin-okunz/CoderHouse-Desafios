<h1>Lista de productos en tiempo real</h1>

<script src="/socket.io/socket.io.js"></script>
<table>

<tr>
    <th>Nombre</th>
    <th>precio</th>
    <th>Stock</th>
    <th>Id</th>
</tr>


{{#each prod}}
<tr>
    <td>{{this.nombre}}</td>
    <td>{{this.precio}}</td>
    <td>{{this.stock}}</td>
    <td>{{this.id}}</td>
</tr>
{{/each}}


</table>
<script src="/js/index.js", type="module"></script>